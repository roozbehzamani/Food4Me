    <script>
        function EditShiftTime() {
            var firstStartShift = $('#timepicker').val();
            var endStartShift = $('#timepicker1').val();
            var firstSecendShift = $('#timepicker2').val();
            var endSecendShift = $('#timepicker3').val();

            $.ajax({
                type: 'POST',
                dataType: 'Json',
                url: '@Url.Action("EditShiftTime", "Admin")',
                data: { fss: firstStartShift, ess: endStartShift, fss2: firstSecendShift, ess2: endSecendShift },
                error: function (err) {
                    alert(err.status);
                }
            }).done(function (result) {
                if (result == 1) {
                    alert("با موفقیت اعمال شد");
                } else {
                    alert("خطا، مجددا تلاش کنید");
                }
            });
